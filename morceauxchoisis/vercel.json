{
  "version": 2,
  "projects": {
    "frontend": {
      "root": "frontend"
    },
    "backend": {
      "root": "backend"
    }
  },
  "builds": [
    {
      "src": "backend/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend$1"
    }
  ]
}
